.createnew {
    background-color: #084FA1;
    color: #fff !important;
    padding: 3px 8px;
    border-radius: 0px;
    cursor: pointer;
    margin-left:0%;
    font-size: 14px;
}

.btn-cancel {
    background-color: #eee;
    color: #fff !important;
    padding: 3px 10px;
    border-radius: 0px !important;
    cursor: pointer;
    border: 0px;
}
.btn-create {
    background-color: #084FA1;
    color: #fff !important;
    padding: 3px 10px;
    border-radius: 0px;
    cursor: pointer;
}
select, input[type="text"], input[type="date"] {
    height: 26px;
    padding-left: 5px !important;
}
.create_list div {
    margin-bottom: 5px;
}
.caret-5 {
    left: 518px;
}
.fa-caret-up {
    position: absolute;
    top: -18px;
    font-size: 26px;
    color: #084FA1;
}
#Div5 {
    border: 1px solid #eee;
    padding: 10px;
    background-color: #fff;
}
#Div5 > div:nth-child(1) {
    border-bottom: 1px solid #eee;
    margin-bottom: 10px;
}
.viewdet_close {
    position: relative;
    top: 18px;
    cursor: pointer;
}

:host /deep/ .ui-inputtext{height:26px;width:297px;}

:host /deep/ .calendar .ui-inputtext{width:273px !important;}

.lnk_view{color:#084FA1 !important;}

:host /deep/ .table th {padding: .35rem .75rem;}

:host /deep/ .ui-corner-all{border-radius:0px;}

:host /deep/ .ui-widget.ui-button, .ui-button{border: 1px solid #084FA1;background: #084FA1;}

:host /deep/ .nav-tabs .nav-link{border-radius:0px;}