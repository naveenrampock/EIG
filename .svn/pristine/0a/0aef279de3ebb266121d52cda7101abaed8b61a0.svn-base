.caseLazy::-webkit-scrollbar {
    width: 4px;
}

.caseLazy::-webkit-scrollbar-track {
    box-shadow: inset 0 0 5px grey; 
    border-radius: 30px;
}
 
.caseLazy::-webkit-scrollbar-thumb {
    background: #084FA1; 
    border-radius: 20px;
}

.caseLazy::-webkit-scrollbar-thumb:hover {
    background: #084FA1; 
}
.caseLazy{
    overflow-y: auto;height: 510px;overflow-x: hidden;
}
.moreComments{
    overflow-y: auto;height: 300px;overflow-x: hidden;
}


