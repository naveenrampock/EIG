.inner_div > div .fa-plus{
    padding: 5px 7px;
    background-color: #084FA1;
    color: #fff;
    border-radius: 0px;
    cursor: pointer
}

.inner_div > div >div:nth-child(1){
    display: block;
    clear: both;
}
.inner_div > div >div:nth-child(1) > a:nth-child(1){
    display: block;
    float: left;
} 
.inner_div > div >div:nth-child(1) > a:nth-child(3){
    float: right;
} 
.inner_div > div >div:nth-child(2){
    clear: both;
}

