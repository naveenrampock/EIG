}

.case-popover .fa-plus, .case-popover .fa-trash, .case-popover .fa-eye, .case-popover .fa-info-circle{padding:7px;background-color:#084FA1;color:#fff;margin-left: 10px;border-radius: 5px;} 

:host /deep/ table th{text-align: center;background-color: #0847a1;color: #fff;}

