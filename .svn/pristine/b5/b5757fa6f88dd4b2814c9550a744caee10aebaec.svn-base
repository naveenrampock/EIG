          <hr class="g-brd-gray-light-v4 g-my-25">

          <div class="text-sm-right">
            <a class="btn u-btn-darkgray rounded-0 g-py-12 g-px-25 g-mr-10" href="#">{{'UserSecuritySettingsModule.Cancel' | translate}}</a>
            <button class="btn u-btn-primary rounded-0 g-py-12 g-px-25" (click)="saveSecuritySettings()" [disabled]="!privacyForm.valid">{{'UserSecuritySettingsModule.SaveChanges' | translate}}</button>
          </div>

        </div>
