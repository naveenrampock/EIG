.legend-like-labels label {
    width: 100%;
    margin-left: 16px;
    margin-bottom: 0px;
    padding: 4px 15px;
    font-size: 11px;
    color: #084FA1;
}

.ebg-white > div {
    overflow: auto;
}

.legend-like-labels> div, .legend-like-labels> div > div, .legend-like-labels> div > div> div {
    margin-bottom: 6px;
}

.emr-labels label{font-size:13px;font-weight:normal;}

.pat-calendar{width:48%;}

:host /deep/ .pat-calendar .ui-inputtext{width:100%;}

.m-t10per{margin-top:10%;}

.fa-calendar{position: absolute;right: 25px;top: 8px;}

.fa-caret-down{position: absolute;top: 8px;right: 10px;}

.addbtn {
    float: right;
    position: relative;
    top: -22px;
}

.delbtn {
    position: relative;
    top: -20px;
}

.error {
    position: relative;
    float: left;
}

:host /deep/ .ui-datepicker{width:18.7em;}

.manage{ width: 290px !important;margin-left: 8px;}

div.disabled {cursor: no-drop !important;}

:host /deep/ .ui-corner-all {border-radius: 0px;}

:host /deep/ .ui-widget.ui-button, .ui-button {border: 1px solid #084FA1;color: #FFFFFF;background: #084FA1;}

.emr-details input[type=text]{ padding: 0px 20px 0px 8px;}

.trash{font-size: 16px;color: red;padding-top: 6px;}

input[type="text" i]:disabled,select:disabled{background-color:#f5f5f5;}

:host /deep/ .ui-widget:disabled{opacity:1;background-color: #f5f5f5;}

:host /deep/ .ui-paginator .ui-paginator-page.ui-state-active {background: #0847a1;color: #ffffff;border-color: #0847a1;}

:host /deep/ .ui-corner-all {border-radius: 0px;}