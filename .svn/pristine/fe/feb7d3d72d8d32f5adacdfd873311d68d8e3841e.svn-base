/* New Login Button */
.socialBtn {
    box-sizing: border-box;
    position: relative;
    margin: 0.2em;
    padding: 0px 15px 0 15px;
    border: none;
    text-align: center;
    line-height: 34px;
    white-space: nowrap;
    border-radius: 0.2em;
    font-size: 14px;
    color: #FFF;
    width: 40%;
    height: 30px;
}
.socialBtn b{
   position: relative;
  bottom: 3px;}
.loginBtn {
    box-sizing: border-box;
    position: relative;
    margin: 0.2em;
    padding: 0px;
    border: none;
    text-align: center;
    line-height: 34px;
    white-space: nowrap;
    font-size: 16px;
    color: #FFF;
    width: 75%;
    height: 35px;
     cursor:pointer;
}
.loginBtn:focus {
  outline: none;
}
.loginBtn:active {
  box-shadow: inset 0 0 0 32px rgba(0,0,0,0.1);
}
.loginBtn--facebook {

    background-color: #3B5998;
    background-image: linear-gradient(#4C69BA, #3B55A0);
    text-shadow: 0 -1px 0 #3B5998;
  }

  .loginBtn--facebook:hover,
  .loginBtn--facebook:focus {
    background-color: #5B7BD5;
    background-image: linear-gradient(#5B7BD5, #4864B1);
  }
  
  /* Google */
.loginBtn--google {
    background: #DD4B39;
   
  }

  .loginBtn--google:hover,
  .loginBtn--google:focus {
    background: #E74B37;
  }

  .social {
    margin-top:10px;
  }

  .social .loginBtn{
    width: 90% !important;text-align:left;
  }

  .social > div{
    width: 50%;
    float: left;
  }

  .close {
    float: right;
    position: relative;
    top: 9px;
    color:#fff;
    right: 15px;
    opacity: 1;
  }

  :host /deep/ .ui-dialog.ui-widget .ui-dialog-titlebar {
    padding: 0.5em 1.5em;
}

:host /deep/ .ui-dialog .ui-dialog-titlebar-icon{
  position: relative;left: 10px;top: 4px;
}

.fa-facebook,.fa-google{font-size: 20px;padding-left: 0px;position: relative;top: 2px;}

.fa + b{font-weight:normal;padding-left: 10px;}

.social .fa + b{font-weight:normal;padding-left: 20px;}

.social .fa-facebook,.social .fa-google{padding-left: 15px;}

:host /deep/ .ui-widget-content{width:350px !important;}

:host /deep/ .confirm .ui-widget-content{width:500px !important;}

.fa-google + b{position: relative;right: 10px;}

.fa-google {right:7px;}

.delete{padding: 5px 20px;}

.disabled {
  pointer-events: unset;
  cursor: no-drop !important;
  opacity: 0.5;
}

