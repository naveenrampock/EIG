select, input[type="text"], input[type="date"]{height:30px;}

.add_lookup{padding: 9px 10px !important;position: relative;top: 1px;border-radius: 0px;background-color: #32c5d2;}

.fa-plus{margin-right:0px;color:#fff;}

.u-nav-v1-1.u-nav-primary .nav-link.active {color: #fff; background-color: #32c5d2;}

.g-brd-primary{border-color: #32c5d2 !important;}

